{
  "dependencies": {
    "assemblyscript": "^0.27.27"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  },
  "scripts": {
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild:wasi": "asc assembly/index.ts --use abort=assembly/index/abort --target wasm32-wasi -o build/wasi.wasm",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "test": "tsc -p tests --outDir ./tests/out && node --experimental-specifier-resolution=node tests/out/tests",
    "start": "npx serve ."
  },
  "devDependencies": {
    "assemblyscript": "^0.27.27"
  }
}